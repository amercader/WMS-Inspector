<?xml version="1.0"?>

<?xml-stylesheet href="chrome://global/skin/" type="text/css"?> 
<?xml-stylesheet href="chrome://wmsinspector/content/wmsinspector.css"?>

<!DOCTYPE overlay SYSTEM "chrome://wmsinspector/locale/strings.dtd">

<dialog xmlns:html="http://www.w3.org/1999/xhtml"
        xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
				id="wiAddParameterDialog"
				style="width: 300px; height: 100px;"
				title="&wmsinspector.addparameter.title;"
                                onload="WMSInspector.AddParameterDialog.init();"
                                ondialogaccept="WMSInspector.AddParameterDialog.onAccept();"
				buttons="accept,cancel">

    <script type="application/x-javascript" src="chrome://wmsinspector/content/utils.js"/>
    <script type="application/x-javascript" src="chrome://wmsinspector/content/addParameterDialog.js"/>
	
	
    <stringbundleset id="stringbundleset">
        <stringbundle id="wiStringBundle" src="chrome://wmsinspector/locale/wmsinspector.properties"/>
    </stringbundleset>

    <groupbox>
        <label value="Name" />
        <textbox id="wiParamName" class="padded" flex="1" />
        <label value="Value" />
        <textbox id="wiParamValue" class="padded" flex="1" />
    </groupbox>

</dialog>
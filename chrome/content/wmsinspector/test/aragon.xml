            <Keyword>Servicios</Keyword>
        </KeywordList>
        <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="http://sitar.aragon.es/AragonBase_WMS" xlink:type="simple"/>
